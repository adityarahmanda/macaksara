{
  "name": "macaksara",
  "version": "1.0.1",
  "description": "Website permainan kuis untuk menguji kemampuan membaca aksara Jawa yang didesain secara interaktif dan menyenangkan.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adityarahmanda/macaksara.git"
  },
  "keywords": [
    "macaksara",
    "aksara, jawa",
    "kuis",
    "membaca",
    "latihan",
    "interaktif"
  ],
  "author": "adityarahmanda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adityarahmanda/macaksara/issues"
  },
  "homepage": "https://github.com/adityarahmanda/macaksara#readme",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "css-compile": "sass --style expanded --source-map --embed-sources --no-error-css --quiet assets/scss/_aksara.scss:assets/css/aksara.css",
    "css-minify": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output assets/css/ --batch --batch-suffix \".min\" \"assets/css/*.css\" \"!assets/css/*.min.css\" \"!assets/css/*rtl*.css\""
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/sitemap": "^2.4.0",
    "@popperjs/core": "^2.11.2",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "clean-css-cli": "^5.6.0",
    "core-js": "^3.25.3",
    "date-fns": "^2.28.0",
    "nuxt": "^2.15.8",
    "xss": "^1.0.11"
  },
  "devDependencies": {
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.2.1",
    "sass": "1.32.13",
    "sass-loader": "^10.2.1"
  }
}
